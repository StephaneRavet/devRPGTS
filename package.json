{
  "name": "devrpg-monorepo",
  "private": true,
  "workspaces": [
    "frontend",
    "backend",
    "shared"
  ],
  "scripts": {
    "dev:backend": "npm run dev --workspace backend",
    "dev:frontend": "npm run dev --workspace frontend",
    "dev": "npm-run-all --parallel --silent --print-label dev:backend dev:frontend"
  },
  "devDependencies": {
    "@types/jquery": "^3.5.32",
    "@types/node": "^22.9.3",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.7.2"
  }
}
